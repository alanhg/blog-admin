    "build:backend": "cd backend && npm run build",
    "build:all": "npm run build:front && npm run build:backend && mv ./frontend/dist .",
    "bundle": "mkdir -p bundle/dist && cp -r frontend/dist/blog-admin/* bundle/dist && cp -r backend/dist/* bundle && cp backend/package*.json bundle"
